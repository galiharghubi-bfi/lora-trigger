{
  "dependencies": {
    "arangojs": "^10.1.2",
    "dayjs": "^1.11.19"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0"
  },
  "name": "lora-trigger",
  "version": "1.0.0",
  "main": "trigger-survey.js",
  "scripts": {
    "survey": "node triggers/trigger-survey.js",
    "start": "bun index.js",
    "ca": "bun start underwriting ca",
    "survey-ndf2w": "bun start survey ndf2w",
    "survey-ndf2w-vd": "bun start survey ndf2w actor=vd",
    "survey-ndf2w-vd-bv": "bun start survey ndf2w actor=vd_branch_visit",
    "survey-ndf4w": "bun start survey ndf4w",
    "survey-ndf4w-vd": "bun start survey ndf4w actor=vd",
    "survey-ndf4w-vd-bv": "bun start survey ndf4w actor=vd_branch_visit",
    "automation-low": "bun start automation ndf4w-full riskLevel=low",
    "automation-low-vd": "bun start automation ndf4w-full riskLevel=low actor=vd",
    "automation-low-vd-bv": "bun start automation ndf4w-full riskLevel=low actor=vd_branch_visit",
    "automation-high": "bun start automation ndf4w-full riskLevel=high",
    "automation-high-vd": "bun start automation ndf4w-full riskLevel=high actor=vd",
    "automation-high-vd-bv": "bun start automation ndf4w-full riskLevel=high actor=vd_branch_visit",
    "automation-ca-no-revision": "bun start automation ca-full",
    "build": "bun build --compile --target=bun-darwin-arm64 ./index.js --minify --outfile lora-trigger"
  },
